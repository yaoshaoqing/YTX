<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fadingEdge="vertical"
    android:scrollbarStyle="insideInset"
    android:scrollbars="vertical">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#ffffff"

        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="210dp"
            android:background="#ffffff">

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="210dp" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="@dimen/x14"
            android:layout_marginRight="@dimen/x10"
            >
        <RelativeLayout
            android:id="@+id/xiaoxi"
            style="@style/personal_Relative"
            >
            <ImageView
                android:id="@+id/xiaoxiImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/xiaoxi2x" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/xiaoxiImg"
                android:layout_gravity="center"
                android:text="消息中心"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />

        <RelativeLayout
            android:id="@+id/order"
            style="@style/personal_Relative">

            <ImageView
                android:id="@+id/orderImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/wodedingdan2x" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/orderImg"
                android:gravity="center_vertical"
                android:text="我的订单"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />
        <RelativeLayout
            android:id="@+id/favorite"
            style="@style/personal_Relative">

            <ImageView
                android:id="@+id/favoriteImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/shoucang2x" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/favoriteImg"
                android:gravity="center_vertical"
                android:text="我的收藏"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />
        <RelativeLayout
            android:id="@+id/coupon"
            style="@style/personal_Relative">

            <ImageView
                android:id="@+id/couponImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/youhuiquan2x" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/couponImg"
                android:gravity="center_vertical"
                android:text="我的优惠券"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="20dp"
            android:background="#f0f0f0"></LinearLayout>

        <RelativeLayout
            android:id="@+id/info"
            style="@style/personal_Relative">

            <ImageView
                android:id="@+id/infoImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/gerenxinxi2x" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/infoImg"
                android:gravity="center_vertical"
                android:text="个人信息"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />
        <RelativeLayout
            android:id="@+id/set"
            style="@style/personal_Relative">

            <ImageView
                android:id="@+id/setImg"
                style="@style/personal_Relative_first_img"
                android:src="@drawable/shezhi2x" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/setImg"
                android:gravity="center_vertical"
                android:text="设置"
                android:textSize="15sp" />

            <ImageView
                style="@style/personal_Relative_end_img"
                android:src="@drawable/jinru2x" />
        </RelativeLayout>
        <View
            style="@style/underline"
            />
    </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="49dp"></LinearLayout>
    </LinearLayout>
</ScrollView>